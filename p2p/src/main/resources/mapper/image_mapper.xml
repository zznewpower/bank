        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mall.dao.ImageDao">
    <insert id="ImageAdd" parameterType="com.p2p.beans.Image">
        insert into commodity_img(imgid,url,commsku) values(#{imgid},#{url},#{commsku})
    </insert>
</mapper>